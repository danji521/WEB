<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>基本输出</title>
	</head>
	<body>
		<h1>基本输出</h1>
		<!-- 模板-->
		<p>{{ name}}</p>
		<p>{{age}}</p>
		<p>{{content}}</p>
		<!-- 标准语法 -->
		<<p>{{ name}}</p>
		<p>{{age}}</p>
		<p>{{ 1 + 1 }}</p>
		<p>{{ 1 + 1 == 2 ? '相等' : '不相等' }}</p>
		<p>{{ content }}</p>
		<p>{{@ content }}</p>
	</body>
</html>
